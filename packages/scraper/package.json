{
  "name": "@sneu/scraper",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "exports": {
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./update": {
      "types": "./dist/update/main.d.ts",
      "default": "./dist/update/main.js"
    },
    "./notifs": {
      "types": "./dist/update/notifs.d.ts",
      "default": "./dist/update/notifs.js"
    },
    "./generate": {
      "types": "./dist/generate/main.d.ts",
      "default": "./dist/generate/main.js"
    },
    "./upload": {
      "types": "./dist/upload/main.d.ts",
      "default": "./dist/upload/main.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./schemas": {
      "types": "./dist/schemas/scraper/banner-cache.d.ts",
      "default": "./dist/schemas/scraper/banner-cache.js"
    }
  },
  "dependencies": {
    "consola": "^3.4.2",
    "drizzle-orm": "^0.45.1",
    "html-entities": "^2.6.0",
    "node-html-parser": "^7.0.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@sneu/db": "workspace:*",
    "@sneu/tsconfig": "workspace:*",
    "@types/node": "^25.1.0",
    "tsx": "^4.21.0",
    "turbo": "^2.8.0",
    "typescript": "^5.9.3"
  }
}
