{
  "name": "@sneu/scraper",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "test": "node --test --import tsx ./**/*.test.ts",
    "scrape:gen": "node --import tsx ./src/scripts/generate.ts",
    "scrape:up": "node --env-file .env --import tsx ./src/scripts/upload.ts"
  },
  "exports": {
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./update": {
      "types": "./dist/update/main.d.ts",
      "default": "./dist/update/main.js"
    },
    "./notifs": {
      "types": "./dist/update/notifs.d.ts",
      "default": "./dist/update/notifs.js"
    }
  },
  "dependencies": {
    "citty": "^0.2.1",
    "consola": "^3.4.2",
    "drizzle-orm": "^0.45.1",
    "html-entities": "^2.6.0",
    "node-html-parser": "^7.0.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@sneu/db": "workspace:*",
    "@sneu/eslint-config": "workspace:*",
    "@sneu/tsconfig": "workspace:*",
    "@types/he": "^1.2.3",
    "@types/node": "^25.2.3",
    "eslint": "^10.0.0",
    "nock": "^14.0.11",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
