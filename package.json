{
  "name": "sneu",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "db:generate": "turbo run db:generate --filter=@sneu/db",
    "db:migrate": "turbo run db:migrate --filter=@sneu/db",
    "db:push": "turbo run db:push --filter=@sneu/db",
    "db:studio": "turbo run db:studio --filter=@sneu/db",
    "scrape:gen": "turbo run scrape:gen --filter=@sneu/scraper",
    "scrape:up": "turbo run scrape:up --filter=@sneu/scraper"
  },
  "devDependencies": {
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "turbo": "^2.8.0"
  },
  "packageManager": "pnpm@10.27.0",
  "engine": {
    "node": "24.x"
  }
}
